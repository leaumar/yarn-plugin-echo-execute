        "@yarnpkg/core": "2.4.0",
        "eslint": "7.27.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "npm-run-all": "4.1.5",
        "prettier": "2.3.0",
        "prettier-plugin-package": "1.3.0",
        "rimraf": "3.0.2",
        "typescript": "4.2.4"
    }
